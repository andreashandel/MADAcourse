@Article{atmar08,
  author      = {Atmar, Robert L. and Opekun, Antone R. and Gilger, Mark A. and Estes, Mary K. and Crawford, Sue E. and Neill, Frederick H. and Graham, David Y.},
  title       = {Norwalk virus shedding after experimental human infection.},
  journal     = {Emerg Infect Dis},
  year        = {2008},
  volume      = {14},
  number      = {10},
  pages       = {1553--1557},
  month       = {Oct},
  abstract    = {Noroviruses are the most common cause of viral gastroenteritis in the United States. To determine the magnitude and duration of virus shedding in feces, we evaluated persons who had been experimentally infected with Norwalk virus. Of 16 persons, clinical gastroenteritis (watery diarrhea and/or vomiting) developed in 11; symptomatic illness lasted 1-2 days. Virus shedding was first detected by reverse transcription-PCR (RT-PCR) 18 hours after participant inoculation and lasted a median of 28 days after inoculation (range 13-56 days). The median peak amount of virus shedding was 95 x 10(9) (range 0.5-1,640 x 10(9)) genomic copies/g feces as measured by quantitative RT-PCR. Virus shedding was first detected by antigen ELISA approximately 33 hours (median 42 hours) after inoculation and lasted 10 days (median 7 days) after inoculation. Understanding of the relevance of prolonged fecal norovirus excretion must await the development of sensitive methods to measure virus infectivity.},
  doi         = {10.3201/eid1410.080117},
  institution = {Molecular Virology and Microbiology, Baylor College of Medicine, Houston, Texas 77030, USA. ratmar@bcm.tmc.edu},
  keywords    = {Adolescent; Adult; Base Sequence; Caliciviridae Infections, transmission/virology; Clinical Protocols; DNA Primers, genetics; Feces, virology; Gastroenteritis, virology; Humans; Middle Aged; Norwalk virus, genetics/isolation /&/ purification/pathogenicity; RNA, Viral, analysis/genetics; Time Factors; Virus Shedding},
  language    = {eng},
