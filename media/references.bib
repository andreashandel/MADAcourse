}

@Article{atmar96,
  author      = {Atmar, R. L. and Neill, F. H. and Woodley, C. M. and Manger, R. and Fout, G. S. and Burkhardt, W. and Leja, L. and McGovern, E. R. and {Le Guyader}, F. and Metcalf, T. G. and Estes, M. K.},
  title       = {Collaborative evaluation of a method for the detection of Norwalk virus in shellfish tissues by PCR.},
  journal     = {Appl Environ Microbiol},
