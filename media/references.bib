  created         = {2015-10-13},
  doi             = {10.1111/cei.12681},
  issn            = {1365-2249},
  issn-linking    = {0009-9104},
  issue           = {2},
  keywords        = {Adult; Caliciviridae Infections, blood, immunology, virology; Chemokine CCL2, blood, immunology; Cytokines, blood, immunology; Feces, virology; Female; Gastroenteritis, blood, immunology, virology; Host-Pathogen Interactions, immunology; Humans; Interleukin-10, blood, immunology; Interleukin-8, blood, immunology; Male; Norovirus, genetics, immunology, physiology; RNA, Viral, genetics; Reverse Transcriptase Polymerase Chain Reaction; Th1 Cells, immunology, metabolism; Th2 Cells, immunology, metabolism; Time Factors; Tumor Necrosis Factor-alpha, blood, immunology; Young Adult; adaptive immunity; caliciviruses; innate immunity},
