@Article{hall78,
  author    = {Hall, Caroline Breese and Douglas Jr., R. Gordon and Simons, Ruth L. and Geiman, Joyce M.},
  title     = {Interferon production in children with respiratory syncytial, influenza, and parainfluenza virus infections},
  journal   = {The Journal of Pediatrics},
  year      = {1978},
  volume    = {93},
  number    = {1},
  pages     = {28--32},
  month     = jul,
  abstract  = {To better understand the recovery process of infants with lower respiratory tract disease due to respiratory syncytial virus, the production of interferon by 129 children (ages 10 days to 24 months) with {RSV} infection was compared to that of 20 children with influenza (ages 1 to 36 months), and 37 children with parainfluenza virus infection (ages 4 to 66 months). Interferon assays of 285 nasal washes from children with {RSV} revealed that interferon production occurred in only 5 (4\%) of the children. Significantly more children infected with influenza virus, 55\% (P\&lt;0.001), and parainfluenza virus, 30\% (P\&lt;0.001), produced interferon. In addition, the quantity of interferon produced by children with {RSV} (geometric mean titer=2) was significantly less than that of children with influenza ({GMT=26.8}, P\&lt;0.001) and parainfluenza virus ({GMT=23.5}, P\&lt;0.001). In the children infected with {RSV}, in contrast to those with influenza, interferon detection was not associated with diminished shedding of virus.},
  doi       = {10.1016/S0022-3476(78)80594-0},
  file      = {ScienceDirect Snapshot:C:\Users\yan\AppData\Roaming\Mozilla\Firefox\Profiles\jkmdc8jv.default\zotero\storage\N7MA9K7T\S0022347678805940.html:text/html},
